{
    "files.exclude": {
        "**/.vscode": false,
        "**/.yardoc": true,
		"**/vendor": true,
        "**/node_modules": true,
        "**/.vagrant": true,
        "**/.bundle": true,
    },
    "git.ignoreLimitWarning": true,
    "editor.defaultFormatter": "SonarSource.sonarlint-vscode",
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
    }
}
